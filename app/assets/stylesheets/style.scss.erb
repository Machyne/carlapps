@charset "UTF-8";
/*------------------------------------*\
    STYLE.CSS
\*------------------------------------*/
/**
 *
 * sass --watch style.scss:style.min.css --style compressed
 *
 * Here we pull in some variables, include the inuit.css framework, then add our
 * project-specific components afterwards.
 */


/**
 * Setup
 */
@import "vars";
@import "inuit.css/inuit";





/**
 * She’s all yours, cap’n... Begin importing your stuff here.
 */
@import "carl.scss";
@import url("//hello.myfonts.net/count/27b469");
@font-face {
    font-family: 'Nanami';
    font-weight: 100;
    src: url("<%= asset_path('nanamithin.otf') %>") format('opentype');
}

@font-face {
    font-family: 'Nanami';
    font-weight: normal;
    src: url("<%= asset_path('nanami.otf') %>") format('opentype');
}

@font-face {
    font-family: 'Nanami';
    font-weight: bold;
    src: url("<%= asset_path('nanamibold.otf') %>") format('opentype');
}

@font-face {
    font-family: 'Nanami';
    font-weight: lighter;
    src: url('webfonts/27B469_0_0.eot');
    src: url('webfonts/27B469_0_0.eot?#iefix') format('embedded-opentype'),url('webfonts/27B469_0_0.woff') format('woff'),url('webfonts/27B469_0_0.ttf') format('truetype');
}

@font-face {
    font-family: 'OpenSans';
    font-weight: normal;
    src: url("<%= asset_path('OpenSans-Regular.ttf') %>") format('truetype');
}